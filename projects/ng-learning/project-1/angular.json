{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "project-1": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/project-1",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
                "src/data",
                "src/images"
                
            ],
            "styles": [
              "src/styles/medibooks-styles.css",
              "src/js/bootstrap-5.1.3-dist/css/bootstrap.min.css",
              "src/styles.css"
            ],
            "scripts": [
              "src/js/bootstrap-5.1.3-dist/js/bootstrap.bundle.min.js"
            ],
            "server": "src/main.server.ts",
            "outputMode": "server",
            "ssr": {
              "entry": "src/server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "project-1:build:production"
            },
            "development": {
              "buildTarget": "project-1:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "host": "0.0.0.0",
            "port": 4200
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
                 "src/data",
                 "src/images"
                 
            ],
            "styles": [
              "src/styles/medibooks-styles.css",
              "src/js/bootstrap-5.1.3-dist/css/bootstrap.min.css",
              "src/styles.css"
            ],
            "scripts": [
              "src/js/bootstrap-5.1.3-dist/js/bootstrap.bundle.min.js"
            ]
          }
        }
      }
    }
  },
  "serve-ssr": {
    "builder": "@nguniversal/builders:ssr-dev-server",
    "configurations": {
      "development": {
        "browserTarget": "project-1:build:development",
        "serverTarget": "project-1:server:development"
      },
      "production": {
        "browserTarget": "project-1:build:production",
        "serverTarget": "project-1:server:production"
      }
    },
    "defaultConfiguration": "development",
    "options": {
      "host": "0.0.0.0",
      "port": 4200,
      "timeout": 120000
    }
  },
  "server": {
    "builder": "@angular-devkit/build-angular:server",
    "options": {
      "outputPath": "dist/project-1-server",
      "main": "src/main.server.ts",
      "tsConfig": "tsconfig.server.json"
    },
    "configurations": {
      "development": {
        "outputHashing": "media",
        "sourceMap": true,
        "optimization": false
      },
      "production": {
        "outputHashing": "media",
        "sourceMap": false,
        "optimization": true
      }
    },
    "defaultConfiguration": "production"
  },
  "cli": {
    "analytics": false
  }
}